# Server configuration
PORT=3000

# Google Cloud TTS
# Set path to JSON credentials file or provide JSON directly
GOOGLE_APPLICATION_CREDENTIALS=path/to/google-credentials.json
# Alternative: GOOGLE_CREDENTIALS={"type":"service_account",...}

# OpenAI TTS
OPENAI_API_KEY=your_openai_api_key

# ElevenLabs TTS
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Amazon Polly
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# Cache settings
CACHE_ENABLED=true
CACHE_EXPIRY=86400 # 24 hours in seconds

# Azure API configuration
AZURE_API_KEY=your_azure_api_key
AZURE_REGION=eastus

# Baidu API configuration
BAIDU_APP_ID=your_baidu_app_id
BAIDU_API_KEY=your_baidu_api_key
BAIDU_SECRET_KEY=your_baidu_secret_key

# Xunfei API configuration
XUNFEI_APP_ID=your_xunfei_app_id
XUNFEI_API_KEY=your_xunfei_api_key
XUNFEI_API_SECRET=your_xunfei_api_secret

# Aliyun API configuration
ALIYUN_ACCESS_KEY_ID=your_aliyun_access_key_id
ALIYUN_ACCESS_KEY_SECRET=your_aliyun_access_key_secret 